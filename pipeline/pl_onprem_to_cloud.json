{
	"name": "pl_onprem_to_cloud",
	"properties": {
		"activities": [
			{
				"name": "OnPremToCloud",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "ds_azure_blob_customer_csv_target",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "onprem_to_cloud"
		},
		"annotations": []
	}
}